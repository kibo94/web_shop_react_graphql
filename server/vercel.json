{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json", 
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"  
        }
      },
      {
        "src": "index.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/graphql", "dest": "/index.js" },
      { "src": "/(.*)", "dest": "/client/build/index.html" }
    ]
  }