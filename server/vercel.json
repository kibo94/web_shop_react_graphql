{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",  
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/buildsss"  
        }
      },
      {
        "src": "api/indexsss.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/graphql", "dest": "/api/indexssss.js" },
      { "src": "/(.*)", "dest": "/client/build/index.html" }
    ]
  }
  